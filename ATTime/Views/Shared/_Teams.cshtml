@model ATTime.Models.Team
@{
    ViewBag.Title = "_Teams";
}

<h1>@ViewBag.testTeamId</h1>


<div class="">
    @using (Html.BeginForm("SelectTeam", "Register", FormMethod.Post))
    {
        <input type="hidden" name="teamid" value="@Model.TeamId" />
        <button type="submit" class="btn btn-default m-1 p-1 btn-primary">@Model.TeamName</button>
    }
    <hr />

    @using (Html.BeginForm("student", "TeacherView", FormMethod.Post))
    {
        <input type="hidden" name="teamid" value="@Model.TeamId" />
        <button type="submit" class="btn btn-default m-1 p-1 btn-basic">Students @ATTime.Models.ViewModel.Studencount.student_count(@Model.TeamId)</button>
    }
</div>




